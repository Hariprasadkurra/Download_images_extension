{
  "manifest_version": 3,
  "name": "Image Downloader",
  "version": "1.0",
  "description": "Download all images from the current webpage.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "icon16.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png"
          }
  }
}
